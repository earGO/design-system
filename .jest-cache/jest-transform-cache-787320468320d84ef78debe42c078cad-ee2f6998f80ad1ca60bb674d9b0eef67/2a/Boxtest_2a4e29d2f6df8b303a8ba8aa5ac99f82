"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");

var _react = _interopRequireDefault(require("react"));

var _reactTestRenderer = _interopRequireDefault(require("react-test-renderer"));

var _Box = _interopRequireDefault(require("../src/Box"));

var _theme = _interopRequireDefault(require("@design-system/theme"));

describe('Box', function () {
  test('renders', function () {
    var json = _reactTestRenderer.default.create(_react.default.createElement(_Box.default, null)).toJSON();

    expect(json).toMatchSnapshot();
  });
  test('width prop sets width', function () {
    var json = _reactTestRenderer.default.create(_react.default.createElement(_Box.default, {
      width: 1 / 2
    })).toJSON();

    expect(json).toMatchSnapshot();
    expect(json).toHaveStyleRule('width', '50%');
  });
  test('m prop sets margin', function () {
    var json = _reactTestRenderer.default.create(_react.default.createElement(_Box.default, {
      m: 2
    })).toJSON();

    expect(json).toMatchSnapshot();
    expect(json).toHaveStyleRule('margin', _theme.default.space[2] + 'px');
  });
  test('p prop sets padding', function () {
    var json = _reactTestRenderer.default.create(_react.default.createElement(_Box.default, {
      p: 2
    })).toJSON();

    expect(json).toMatchSnapshot();
    expect(json).toHaveStyleRule('padding', _theme.default.space[2] + 'px');
  });
  test('color prop sets color', function () {
    var json = _reactTestRenderer.default.create(_react.default.createElement(_Box.default, {
      color: "blue"
    })).toJSON();

    expect(json).toMatchSnapshot();
    expect(json).toHaveStyleRule('color', _theme.default.colors.blue);
  });
  test('bg prop sets background color', function () {
    var json = _reactTestRenderer.default.create(_react.default.createElement(_Box.default, {
      bg: "green"
    })).toJSON();

    expect(json).toMatchSnapshot();
    expect(json).toHaveStyleRule('background-color', _theme.default.colors.green);
  });
  test('align prop triggers warning', function () {
    console.error = jest.fn();

    var json = _reactTestRenderer.default.create(_react.default.createElement(_Box.default, {
      align: "center"
    })).toJSON();

    expect(console.error.mock.calls.toString().indexOf('The Box `align` prop will deprecated. Please use Text instead.') !== -1);
    console.error.mockRestore();
  });
});
