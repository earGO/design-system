{
  "name": "@design-system/icons",
  "version": "1.0.0",
  "description": "Icons based on Material Design Icons",
  "main": "components/index.js",
  "src": "components/index.js",
  "scripts": {
    "prepare": "npm run clean && npm run build:svg && npm run build:index && npm run build:copy && npm run build:babel",
    "clean": "rm -rf node_modules components lib",
    "build:svg": "svgr svg -d components",
    "build:index": "node ./create-index.js",
    "build:copy": "cp src/Icon.js src/Svg.js src/index.js components",
    "build:babel": "babel components -d lib",
    "test": "jest",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "cd storybook && build-storybook"
  },
  "author": "Priceline",
  "license": "MIT",
  "dependencies": {
    "lodash.upperfirst": "^4.3.1",
    "styled-system": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.5.5",
    "@storybook/react": "^5.1.9",
    "@svgr/cli": "^4.1.0",
    "babel-loader": "^8.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-links": "^5.0.10",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addons": "^5.0.10"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "styled-components": ">=3.x || >=4.x"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "../../test-setup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ]
  },
  "engines": {
    "node": ">=8.0.0 <9.0.0 || >=10.0.0",
    "npm": ">=5.10.0 || >=6.0.0"
  }
}
