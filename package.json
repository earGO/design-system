{
  "name": "@ursip/design-system",
  "version": "1.2.0",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/utils",
      "packages/theme",
      "packages/atoms",
      "packages/molecules",
      "packages/cells"
    ],
    "nohoist": [
      "**/webpack-dev-server",
      "**/babel-loader"
    ]
  },
  "scripts": {
    "start": "lerna parallel -- rollup -c -w",
    "build-utils": "cd packages/utils && rollup -c",
    "build-theme": "cd packages/theme && rollup -c",
    "build-atoms": "cd packages/atoms && rollup -c",
    "build-molecules": "cd packages/molecules && rollup -c",
    "build-cells": "cd packages/cells && rollup -c",
    "build": "yarn clean-builds && yarn reinstall && yarn build-utils && yarn build-theme && yarn build-atoms && yarn build-molecules && yarn build-cells",
    "make-patch": "lerna version patch --amend --conventional-commits --create-release gitlab --no-push --changelog-preset react && yarn",
    "make-minor": "lerna version minor --amend --conventional-commits --create-release gitlab --no-push --changelog-preset react && yarn",
    "make-major": "lerna version major --amend --conventional-commits --create-release gitlab --no-push --changelog-preset react && yarn",
    "make-oao-nexus": "oao publish --no-checks --no-confirm --no-git-commit --increment-version-by minor ",
    "styleguide": "styleguidist server --config documentation/config.js",
    "styleguide:build": "styleguidist build --config documentation/config.js",
    "atoms-storybook": "cd packages/atoms && start-storybook -p 9019",
    "cells-storybook": "cd packages/cells && start-storybook -p 9029",
    "molecules-storybook": "cd packages/molecules && start-storybook -p 9039",
    "unit": "jest --watchAll --updateSnapshot",
    "clean-builds": "lerna exec -- rm -rf build",
    "clean": "oao clean",
    "status": "oao status",
    "remove-all": "oao remove-all",
    "reinstall": "oao clean && yarn"
  },
  "dependencies": {
    "@styled-system/theme-get": "^5.0.12",
    "aphrodite": "^2.3.1",
    "color": "^3.1.0",
    "css-color-names": "^1.0.1",
    "file-loader": "^4.0.0",
    "hex-rgb": "^4.1.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "rc-form": "^2.4.1",
    "rc-trigger": "^2.6.2",
    "react-custom-scrollbars": "^4.2.1",
    "react-modal": "^3.8.1",
    "react-select": "^2.3.0",
    "react-tiny-popover": "^3.4.2",
    "react-window": "^1.5.1",
    "rsuite-table": "^3.5.4",
    "ttf-loader": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.6",
    "styled-system": "^3.2.0",
    "ttf-loader": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "7.5.5",
    "@svgr/rollup": "^4.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-react-docgen": "^3.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.1.4",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest-prop-type-error": "^1.1.0",
    "jest-styled-components": "^6.3.3",
    "lerna": "^3.15.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-plugin-clean-css": "^1.5.1",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "oao": "^1.6.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.0",
    "react-styleguidist": "^8.0.6",
    "rollup": "^1.14.6",
    "rollup-plugin-babel": "^4.3.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-url": "^2.1.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.6",
    "styled-system": "^3.2.0",
    "url-loader": "^2.0.1",
    "webpack": "^4.29.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "files": [
    "build"
  ],
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "testMatch": [
      "<rootDir>/packages/**/__test__/**/*.js"
    ]
  },
  "publishConfig": {
    "registry": "https://gitlab.ursip.ru/nexus/repository/npm-local/"
  }
}
